
if User.find_by_email("{{alveo_admin_user_email}}").nil?
    role = Role.find_by_name("admin")
    admin = User.new(email:"{{alveo_admin_user_email}}", first_name:"{{alveo_admin_user_first_name}}", last_name:"{{alveo_admin_user_last_name}}")
    admin.role = role
    admin.password = admin.password_confirmation = '{{alveo_admin_user_password}}'
    admin.activate
    admin.save!
end